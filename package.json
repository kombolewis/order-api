{
  "name": "order-api",
  "version": "1.0.0",
  "description": "This is the example from the Book Hands on RESTful Web Services with TypeScript 3",
  "main": "./dist/server.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist && rimraf reports",
    "lint": "gts lint",
    "pretest": "cross-env NODE_ENV=test npm run build && npm run lint",
    "test": "cross-env NODE_ENV=test mocha --reporter spec --compilers ts:ts-node/register test/**/*.spec.ts ",
    "test:mutation": "stryker run",
    "stryker:init": "stryker init",
    "dev": "cross-env PORT=3000 NODE_ENV=dev ts-node ./src/server.ts",
    "prod": "PORT=3000 npm run build && npm run start",
    "tsc": "tsc",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "posttest": "npm run lint"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "order POC",
    "Hands on RESTful Web Services with TypeScript 3",
    "TypeScript 3",
    "Packt Books"
  ],
  "author": "Biharck Muniz Ara√∫jo",
  "license": "MIT",
  "devDependencies": {
    "@stryker-mutator/core": "^6.4.2",
    "@stryker-mutator/mocha-runner": "^6.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.4",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.9",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "gts": "^3.1.1",
    "mocha": "^10.2.0",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.6"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "chai-http": "^4.3.0",
    "express": "^4.18.2"
  }
}
